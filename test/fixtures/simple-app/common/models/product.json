{
  "name": "Product",
  "base": "PersistedModel",
  "idInjection": true,
  "forceId": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "default": "temp"
    },
    "personId": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "person": {
      "type": "belongsTo",
      "model": "Person",
      "foreignKey": "personId",
      "description": "Returns the person associated with a product."
    }
  },
  "methods": {},
  "mixins": {
    "ReadOnly": {
      "status": true
    }
  }
}
